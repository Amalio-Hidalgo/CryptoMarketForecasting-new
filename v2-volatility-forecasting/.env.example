# API Keys Configuration
# Copy this file to .env and fill in your actual API keys

# CoinGecko API - Get free API key from https://coingecko.com/api
COINGECKO_API_KEY=your_coingecko_api_key_here

# Dune Analytics API - Get API key from https://dune.com/
DUNE_API_KEY=your_dune_api_key_here

# LunarCrush API - Get API key from https://lunarcrush.com/
LUNARCRUSH_API_KEY=your_lunarcrush_api_key_here
LUNARCRUSH_BEARER_TOKEN=your_lunarcrush_bearer_token_here

# Deribit API - Get API key from https://deribit.com/
DERIBIT_API_KEY=your_deribit_api_key_here

# X (Twitter) API - Get API keys from https://developer.twitter.com/
X_API_KEY=your_x_api_key_here
X_API_SECRET=your_x_api_secret_here
X_BEARER_TOKEN=your_x_bearer_token_here

# MERLIN API - Get API key from your MERLIN provider
MERLIN_API_KEY=your_merlin_api_key_here
MERLIN_USER_ADDRESS=your_merlin_user_address_here

# Optional: Configure Dask settings
DASK_WORKERS=4
DASK_THREADS_PER_WORKER=5
DASK_MEMORY_LIMIT=8GB